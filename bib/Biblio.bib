@Article{Manton1991,
  author    = {Manton, Kenneth G. and Stallard, Eric and Tolley, H. Dennis},
  title     = {Limits to {Human} {Life} {Expectancy}: {Evidence}, {Prospects}, and {Implications}},
  year      = {1991},
  issn      = {00987921 17284457},
  number    = {4},
  pages     = {603},
  abstract  = {Identifying limits to human life expectancy and life span is difficult because survival is determined by the individual's physiology, exogeneous influences, and their interaction over time. To explore theoretical limits, the authors examine the life expectancy of selected populations with good health behavior and apply a multivariate risk-factor model to longitudinal data. The risk-factor model and the population data produce consistent estimates of a lower bound of the theoretical limit to human life expectancy. The results suggest that such limits may be higher than estimates obtained by extrapolating human mortality trends, which, necessarily, are dependent on historical conditions. The investigation emphasizes the need to use information on individual physiological processes and health changes prior to death, in addition to mortality or 'endpoint' data, in making estimates. Doing so can help, for example, to determine whether current conditions are stable or in flux.},
  annote    = {Population and Development ReviewItem Citation: Population and Development Review, 12/1/1991, Vol. 17, Issue 4, p. 603-637Accession Number: edsjsr.10.2307.1973599; Publication Type: Article; Source: Population and Development Review; Language: English; Publication Date: 19911201; Imprint: The Population Council},
