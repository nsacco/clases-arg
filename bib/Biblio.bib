@Article{Vallot1969,
  author    = {Vallot, Françoise and Roussel, Louis},
  journal   = {Population (French Edition)},
  title     = {La formation de la famille selon les groupes socio-professionnels},
  year      = {1969},
  issn      = {00324663},
  number    = {5},
  pages     = {897--918},
  volume    = {24},
  abstract  = {La section de psychosociologie de l'I.N.E.D. a réalisé, à la fin de l'année 1968, une enquête d'opinion sur la motivation de la famille limitée. Cette recherche a fait l'objet d'un article publié dans "Population" 1969 no 2. Cette étude, avant tout qualitative, s'adressait à des femmes mariées de 18 à 45 ans; l'échantillon composé de 2 299 personnes n'était pas représentatif de l'ensemble des femmes mariées de France, mais était réparti en groupes égaux d'âge, de milieu socio-professionnel et de nombre d'enfants. Le premier article ne portait que sur une partie des données rassemblées au cours de l'enquête. Mme F. Vallot et M. L. Roussel complètent ici l'analyse des résultats et présentent, sur divers aspects de la vie matrimoniale, les opinions et le comportement de la population enquêtée; ils en dégagent, en particulier, des modèles correspondant à divers groupes professionnels. /// At the end of 1968, I.N.E.D.'s psychosociological section has conducted a survey on the reasons of the limited family. This research work, published in "Population" 1969 no. 2, was a qualitative survey and applied to married women between 18 and 45 years old. The sample of 2299 persons was not representative of French married women but was divided between equal groups of age, social status and number of children. The first article delt only with one part of the data gathered by the survey. F. Vallot and L. Roussel complete here the analysis of the results and present the opinions and the attitudes of the sample about the various aspects of its matrimonial life. Then the authors draw models corresponding to various social status categories. /// La sección de psico-sociología del I.N.E.D. ha conducido, al fin del año 1968, una encuesta tocante a la motivación de la familia limitada. Esta investigación, publicada en "Population" 1969 no 2, estaba sobre todo cualitativa y se dirigia a las mujeres casadas que tenian entre 18 y 45 años; la muestra de 2299 personas no estaba representativa del conjunto de las mujeres casadas de Francia pero estaba dividida en grupos iguales de edad, profesión del marido y número de hijos. El primer artículo trataba soló con una parte de los datos reunidos por la encuesta. F. Vallot y L. Roussel completan aquí el análisis de los resultados y presentan las opiniones de las mujeres de la encuesta tocante a los varios aspectos de su vida matrimonial. Luego los autores describen modelos que corresponden a varios grupos profesionales.},
  annote    = {The following values have no corresponding Zotero field:publisher: Institut National d'Études Démographiques},
  file      = {Vallot-1969:files/17604/Vallot-1969.pdf:application/pdf},
  keywords  = {unipersonales},
  timestamp = {2020-09-30},
}

