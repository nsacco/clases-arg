@Article{Burton1992,
  author    = {Burton, Mary Diane and Grusky, David B.},
  journal   = {Contemporary Sociology},
  title     = {A {Quantitative} {History} of {Comparative} {Stratification} {Research}},
  year      = {1992},
  issn      = {00943061},
  number    = {5},
  pages     = {623--631},
  volume    = {21},
  abstract  = {This article presents a quantitative history of comparative stratification research. Comparative research is a growth industry, indeed, nearly half of all articles in the field adopt a comparative orientation and the consensus view seems to be that the discipline should invest yet more heavily in such research. Scholars interested in issues of modernization form a natural constituency for comparative research. Although the reigning debates in the field are old and dated, there is no reason to expect a sudden decline in their popularity and a consequent falloff in comparative research. It is difficult to be equally optimistic about the long tradition of research on cross-national convergence and variability. This unhappy state of affairs might be blamed on the data themselves. It should be obvious that yet further difficulties emerge when the focus shifts to explaining cross-societal variations in stratification. Any additional investments in explanatory work will typically be coupled with disinvestments in other forms of research that may well yield a higher sociological payoff. Indeed, if this trade-off is achieved at the expense of descriptive forms of comparative work, it is doubted that it would be one worth making.},
  annote    = {Burton, Mary Diane 1; Grusky, David B. 1; Affiliations: 1 : Stanford University; Source Info: Sep92, Vol. 21 Issue 5, p623; Thesaurus Term: SOCIOLOGY; Thesaurus Term: DIFFERENCES; Subject Term: COMPARATIVE studies; Subject Term: RESEARCH; Subject Term: AREA studies; Number of Pages: 9p; Illustrations: 1 Chart, 3 Graphs; Document Type: Article},
