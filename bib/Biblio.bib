}

@Article{Becquet2018,
  author    = {Becquet, Valentine and Guilmoto, Christophe Z.},
  journal   = {Population},
  title     = {Le déséquilibre des sexes à la naissance au {Vietnam} : de la hausse rapide à la stabilisation},
  year      = {2018},
  issn      = {9782733220320},
  number    = {3},
  pages     = {543--570},
  volume    = {73},
  abstract  = {Le Vietnam est l’un des pays où la proportion de naissances masculines a récemment augmenté en raison des avortements sélectifs selon le sexe. Les données d'état civil exhaustives faisant défaut, nous recourrons à deux méthodes indirectes à partir de l’enquête intercensitaire de 2014 pour suivre les tendances du rapport de masculinité à la naissance (RMN) et estimer les préférences de sexe en matière de fécondité. Ces méthodes fournissent des résultats similaires qui attestent de la poursuite de la hausse du RMN au Vietnam entamée en 2003. Toutefois, les données annuelles du Bureau national de la statistique montrent que la progression du RMN a atteint un plateau en 2014, situé entre 112 et 113 naissances masculines pour 100 naissances féminines, qui apparaît relativement stable jusqu’aux dernières données provisoires disponibles (2017). Cette analyse met en évidence les disparités du RMN, plus déséquilibré dans les régions du Nord et chez les couples les plus aisés, et la diffusion de la sélection prénatale au sein de ces régions et des différents groupes socioéconomiques sur une période de cinq ans. Les probabilités d’agrandissement permettent également d’examiner les différences de comportements reproductifs en montrant que les couples vietnamiens ont tendance à augmenter leur nombre d’enfants lorsqu’ils n’ont pas eu de fils.},
  annote    = {The following values have no corresponding Zotero field:translated-title: Sex imbalance at birth in Vietnam: Rapid increase followed by stabilizationremote-database-provider: Cairn.info},
  doi       = {10.3917/popu.1803.0543},
  language  = {Fr},
  timestamp = {2020-09-30},
}

