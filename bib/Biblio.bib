@Article{Sacco2016c,
  author    = {Sacco, Nicolás},
  journal   = {Notas de Población},
  title     = {¿{Cuánto} vivieron los nacidos a fines del siglo {XIX} y cuánto vivirán los nacidos a fines del siglo {XX}?},
  year      = {2016},
  number    = {julio-diciembre},
  pages     = {73--100},
  volume    = {103},
  abstract  = {Los objetivos de este trabajo son los de contar una historia de la supervivencia en Argentina que presente métodos y resultados de un estudio de la población que perfile la mortalidad y la experiencia de la sobrevivencia en Argentina de aquellas generaciones nacidas en 1900 y 2000, derivando para ello tablas abreviadas de mortalidad de cohortes. Se intentará, en definitiva responder de forma comparativa, a la  siguiente pregunta: ¿cuánto vivieron los nacidos en 1900 y cuánto vivirán los que nacen 100 años después?   Siguiendo a (Rowland, 1996) la propuesta de este trabajo es la de aplicar una aproximación comparativa para el estudio de esperanza de vida por cohortes. Este método propone elaborar información comparable de la supervivencia de las cohortes en diferentes países y/o regiones para ayudar a llenar la actual brecha que existe en estudios comparativos de mortalidad, evadiendo problemas del ajuste de datos al emplear tablas modelo.},
  annote    = {The following values have no corresponding Zotero field:pub-location: Santiago de Chilepublisher: Comisión Económica para América Latina y el Caribe (CEPAL) Centro Latinoamericano y Caribeño de Demografía (CELADE)},
