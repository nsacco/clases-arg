@Article{Mills2010,
  author    = {Mills, M. and Begall, K.},
  journal   = {Popul Stud (Camb)},
  title     = {Preferences for the sex-composition of children in {Europe}: a multilevel examination of its effect on progression to a third child},
  year      = {2010},
  issn      = {1477-4747 (Electronic) 0032-4728 (Linking)},
  month     = mar,
  number    = {1},
  pages     = {77--95},
  volume    = {64},
  abstract  = {Comparative research on the preferred sex of children in Western societies has generally focused on women only and ignored the role of gender equity and the need for children's economic support in old age. A multilevel analysis extends existing research by examining, for both men and women and across 24 European countries, the effect of the preferred sex-composition of offspring on whether parents have or intend to have a third child. Using the European Social Survey (2004/5), a multilevel (random coefficient) ordered logit regression of that intention (N = 3,323) and a binary logistic multilevel model of the transition to a third child (N = 6,502) demonstrate the presence of a mixed-sex preference. In countries with a high risk of poverty in old age, a preference for sons is found, particularly for men. In societies where there is lower gender equity, both men and women have a significant preference for boys.},
  annote    = {Mills, MelindaBegall, KatiaengEnglandPopul Stud (Camb). 2010 Mar;64(1):77-95. doi: 10.1080/00324720903497081.},
  doi       = {10.1080/00324720903497081},
