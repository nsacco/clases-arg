@Misc{Sacco2015,
  author    = {Sacco, Nicolás},
  title     = {¿{Cierran} los números con la gente adentro? {Principales} resultados de una evaluación general del censo argentino de 2010},
  year      = {2015},
  abstract  = {-Objetivos  El objetivo principal de esta ponencia es presentar los principales resultados de un informe de evaluación global del censo 2010.  Pensado como una ?caja de herramientas? para usuarios que abordan por primera vez, o no, esta fuente de datos y en los no especialistas en demografía que utilizan de forma ocasional datos secundarios censales el objetivo de este informe fue el de analizar, a nivel del total del país y según los propios datos y procedimientos que dieron lugar a la construcción de esos datos, cuáles fueron las limitaciones y alcances de la información disponible y publicada del último censo de población de la Argentina.  La presentación de los resultados del  informe en la ponencia procura complementar e integrar la diversidad de publicaciones del propio INDEC y las investigaciones ya realizadas sobre censo 2010 pero se basa, fundamentalmente, en el procesamiento y análisis de información original.   Orientado en base a los principios y recomendaciones de Naciones Unidas se examinan cada una de las temáticas de la cédula censal de forma comparativa con los censos previos de 1991 y 2001.   Con el fin de presentar las conclusiones y un resumen del informe global, se aborda cada etapa en particular cada etapa, la pre, la censal, y pos censal.  Se hará énfasis en el ciclo de interpretación de resultados a partir de la información publicada al momento de finalización de escritura del informe (marzo de 2015). En primer lugar abordando la cobertura de unidades de análisis (población, vivienda y hogares) y luego se presentan los resultados de la evaluación general de datos de sexo y edad y el análisis de las variables demográficas.},
  address   = {Ciudad de Salta, Provincia de Salta, Argentina},
  annote    = {The following values have no corresponding Zotero field:number: 16-18 de septiembre de 2015publisher: Universidad Nacional de Salta},
  timestamp = {2020-09-30},
}

@Misc{Sacco2015b,
  author    = {Sacco, Nicolás},
  title     = {¿{Cuánto} vivieron los nacidos a fines del siglo {XIX} y cuánto vivirán los nacidos a fines del siglo {XX}? {La} esperanza de vida a partir de una aproximación longitudinal.},
  year      = {2015},
  abstract  = {Los objetivos de este trabajo son los de contar una historia de la supervivencia en Argentina que presente métodos y resultados de un estudio de la población que perfile la mortalidad y la experiencia de la sobrevivencia en Argentina de aquellas generaciones nacidas en 1900 y 2000, derivando para ello tablas abreviadas de mortalidad de cohortes. Se intentará, en definitiva responder de forma comparativa, a la  siguiente pregunta: ¿cuánto vivieron los nacidos en 1900 y cuánto vivirán los que nacen 100 años después?   Siguiendo a (Rowland, 1996) la propuesta de este trabajo es la de aplicar una aproximación comparativa para el estudio de esperanza de vida por cohortes. Este método propone elaborar información comparable de la supervivencia de las cohortes en diferentes países y/o regiones para ayudar a llenar la actual brecha que existe en estudios comparativos de mortalidad, evadiendo problemas del ajuste de datos al emplear tablas modelo.},
  address   = {Ciudad de Salta, Provincia de Salta, Argentina},
  annote    = {The following values have no corresponding Zotero field:number: 16-18 de septiembre de 2015publisher: Universidad Nacional de Salta},
  timestamp = {2020-09-30},
