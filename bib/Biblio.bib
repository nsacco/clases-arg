@InProceedings{Sacco2011b,
  author    = {Sacco, Nicolás},
  title     = {Estructura {Social} {Argentina} 1991-2001},
  year      = {2011},
  publisher = {Cuadernos, N° 39},
  abstract  = {El estudio de la estructura social argentina -enfocado como el análisis global de la evolución del volumen y morfología de las clases- no ha tenido gran continuidad en la bibliografía sociológica. Si esto es preocupante cuando se visualiza el país como una unidad, mucho más grave lo es al nivel de las regiones que dividen el territorio nacional. En este artículo se tratará de establecer los cambios que se operan en la estructura social argentina en el lapso 1991-2001, buscando su explicación en el nuevo modelo de acumulación que se inaugura en 1989. Se trata aquí de una perspectiva diacrónica que consiste en comparar el perfil de la estructura nacional en varios momentos puntuales, tratando de explicar los cambios detectados en función de la situación política y de la naturaleza de las políticas económicas concomitantes. Se busca así detectar el impacto del nuevo modelo al nivel del total del país como unidad geográfica y de las economías regionales, observando la consecuente modificación de sus respectivas estructuras sociales.    Se analizarán las estructuras de clases y movilidad social a partir de una metodología capaz de articular el sistema clasificatorio llamado de ?Condición Socio-Ocupacional? (CSO), instrumento elaborado en uno de los programas más importantes derivados del Censo de 1980 (CFI, 1989), y se evaluará su desempeño analítico para cumplir con los objetivos.},
  annote    = {actas},
