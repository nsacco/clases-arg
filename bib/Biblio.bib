@Article{RodriguezWong1983a,
  author    = {Rodriguez Wong, L. L.},
  journal   = {Inf Demogr.w},
  title     = {Regional fertility levels and trends in {Brazil}--application of own children method to the 1970 census},
  year      = {1983},
  pages     = {99--139},
  volume    = {9},
  abstract  = {The total fertility rate (TFR) estimated for the period of 1930-35 was 6.22 children per mother, which declined to 5.80 during 1940-45, increased slightly to 5.86 during 1950-55, and to 6.0 during 1960-65, to drop to 4.97 during 1970-75. The decline was 17.2\% between 1963 and 1973, 14.3\% between 1943-1973, and 20.1\% between 1933 and 1973 (or an average decline by 1.26 children per woman in 40 years). The evolution of the national and regional levels of fertility was estimated by a unique methodology that utilized the average births per woman with data extracted from censuses and research samples. In particular, the technique of Brass was used with correction for P2/F2 as valid data 5 years prior to taking the information. In this manner the TFR was estimated at 3.5142 based on the 1984 national household survey and specific rates for age groups encompassed the period of 1979-84. In this case it was estimated that 3,311,000 births must have occurred on the average annually during this period. It was only in 1980 that the Civil Register recorded 3,860,000 births. During this same period the hypothesis of constancy, utilizing current fertility rates corrected for P2/F2, yielded 2,745,000 births in the course of 5 years. In recent times fertility in Brazil has been on a declining trend, starting in the 1960s. The intensity of this decline varied according to the sources of information: the Civil Register and the national survey of households. Prior to the 1960s the less developed regions of the north experienced pronounced increases, while the southeastern and southern regions showed declines. From the mid-1960s all regions experienced a forceful decline in fertiltiy levels. The results indicate the convincing nature of the proposed model for estimating the levels and patterns of fertility, and it is particularly useful for other levels of disaggregation.},
  timestamp = {2020-09-30},
}

@Article{RodriguezWong1983,
  author    = {Rodriguez Wong, L. L.},
  journal   = {Inf Demogr},
  title     = {Fertility in {Brazil} (urban and rural)--application of the own children method to the 1970 census},
  year      = {1983},
  pages     = {53--97},
  volume    = {9},
  abstract  = {In a search for better understanding of fertility trends and rates over periods of time, the "own children" method was applied to Brazilian census data. The main objective of the study was definition of the level and pattern of fertility in Brazil by utilizing the own children method on data from the 1970 demographic census of Brazil. The time period covered was the 15 years before the census; thus, the fertility situation of the 1960s is shown in detail, an era for which only sketchy superficial estimates existed. Additionally, the conditions of urban and rural residence of the population are considered. This is a 1st attempt at obtaining fertility differentials by diverse socioeconomic criteria. The results obtained for both the entire country as well as for urban and rural areas coincide with estimates made by using other types of data. It is necessary to emphasize, however, that results from the years immediately before the census should be interpreted with caution because of the lower quality of the data. The method provides abundant information about fertility, permitting clear identification of trends over time and the population sectors responsible for these trends. During 1957-1968 the total fertility rate swings between 6 and 5 children per woman, and is characterized by a steady level during the early part of the period and a tendency to decline that began in 1965. The age groups repeat this behavior, except for the age 20-24 group, which demonstrated a slight tendency to increase up to 1965, when a strong decrease began. The age patterns maintain, in general, the same form throughout the period; the only change recorded was a slight aging of the age structure. The behavior of the urban and rural areas were relatively different. The urban area tended to begin the decline before 1960 and to maintain it up to the end of the period. The rural area, by contrast, registered a tendency to increase which ended in the mid-1960s, afterwards beginning a decline. The fertility levels presented by the 2 regions are markedly different. When the period began, total fertility rates were 5.10 and 7.43 for urban and rural areas respectively. At the end of the decade, equivalent rates were 4.06 and 6.72 children per woman, amplifying the difference between the 2 sectors. The author expresses intent to apply the own children method to other socioeconomic variables beyond the geographic; e.g., education, income, and occupation. The method will also be applied to 1980 Brazilian census data.},
  timestamp = {2020-09-30},
}

