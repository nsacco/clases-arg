@Article{Szreter1984,
  author    = {Szreter, Simon R. S.},
  journal   = {British Journal of Sociology},
  title     = {The genesis of the {Registrar}-{General}'s social classification of occupations},
  year      = {1984},
  issn      = {00071315},
  number    = {4},
  pages     = {522--546},
  volume    = {35},
  abstract  = {Those empirical sociologists in Britain who are concerned with social classification have tended to avoid any detailed evaluation of the long-established and ubiquitous official system. Such a discussion is inhibited by our current state of ignorance regarding its historical origins. An investigation of the exact chronology of its development and elucidation of its author's intentions is, therefore, undertaken. This reveals a complex and protracted genesis, over the first three decades of this century. The final form of the official social classification was determined by the intellectual pre-occupations of its progenitor, T.H.C. Stevenson, in seeking a comprehensive alternative explanation to that originally proposed by hereditarian ‘sociologists’, to account for the phenomenon of fertility decline. Consequently, the now familiar five-graded hierarchy entails certain specific methodological assumptions and theoretical limitations, which, it is suggested, may constitute undesirable conceptual impedimenta for modern sociologists. [ABSTRACT FROM AUTHOR]
