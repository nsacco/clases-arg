@Misc{Sacco2015c,
  author    = {Sacco, Nicolás and Carrera de Sociología, Facultad de Ciencias Sociales},
  title     = {Evaluación general de los datos de mortalidad del censo 2010},
  year      = {2015},
  abstract  = {El artículo evalúa las variables disponibles del último censo de población en lo que a mortalidad refiere. En base a métodos indirectos de estimación ofrece una lectura de la calidad de estas estadísticas en su comparación con censos previos y estadísticas vitales. Aborda de forma separada tanto la mortalidad infantil como adulta a partir de diversas técnicas de estimación demográfica.Los resultados procuran complementar e integrar la diversidad de publicaciones del propio INDEC y las investigaciones ya realizadas sobre censo 2010 pero se basa, fundamentalmente, en el procesamiento y análisis de información original. La comparación de las tasas de mortalidad infantil y adulta con las tasas calculadas a partir de los nacimientos y las defunciones, permite dilucidar, en principio, que estos datos no revelan problemas serios de calidad o cobertura y que el nivel (y la tendencia) indicada por las fuentes son ampliamente compatibles.Sin embargo, a pesar de que los resultados parecen mostrar coherencia, la posibilidad de detectar y cuantificar los errores se mantiene como un punto ciego del artículo hasta tanto y cuanto no se publique la totalidad de la información necesaria para realizar un examen cabal, dado el universo limitado de publicaciones por parte del INDEC.},
  address   = {Buenos Aires, Argentina},
  annote    = {The following values have no corresponding Zotero field:number: 13-17 de julio de 2015},
  timestamp = {2020-09-30},
}

@InProceedings{Sacco2015f,
  author    = {Sacco, Nicolás},
  title     = {Reproducción biológica y reproducción de clases sociales durante la segunda mitad del siglo {XX} en la {Argentina}: ¿dos dimensiones de un mismo fenómeno?},
  year      = {2015},
  publisher = {Universidad Nacional de Río Negro},
  abstract  = {En esta presentación se analizará la contribución de la mortalidad y de la fecundidad a la evolución de la población en la Argentina durante la segunda mitad del siglo XX, contando con el concepto de reproducción, que sintetiza el comportamiento demográfico de las diferentes generaciones y sus implicaciones tanto en el crecimiento de la población como en los factores de movilidad social.  En la primera parte se analizarán las transformaciones experimentadas en la mortalidad y la fecundidad de momento y de las generaciones que explican la evolución de la reproducción demográfica de las generaciones. En las segunda parte  se trata la contribución de los cambios en la estructura de clases sociales, su evolución y su relación con los efectivos de la población desde una perspectiva generacional.    El análisis de los componentes del crecimiento demográfico de la población argentina durante el siglo XX  se considera que es un factor determinante de cambio social, que se prevé que protagonista en el siglo XXI.},
  annote    = {The following values have no corresponding Zotero field:number: 13-15 mayopub-location: Bariloche, Argentina},
  timestamp = {2020-09-30},
}

