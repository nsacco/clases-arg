  annote    = {70314510Revista Espanola de Investigaciones Sociologicas},
  file      = {H. Goldthorpe-2012:files/22499/H. Goldthorpe-2012.pdf:application/pdf},
  keywords  = {Movilidad social, Clase social, Desigualdad social, Economistas, Economists, Epidemiologists, Epidemiólogos, Estatus social, Social class, Social inequality, Social mobility, Social status},
  timestamp = {2020-09-30},
