@Article{Roussel1986,
  author    = {Roussel, Louis},
  journal   = {Population},
  title     = {L'évolution récente de la structure des ménages dans quelques pays industriels},
  year      = {1986},
  issn      = {0032-4663},
  pages     = {913--934},
  abstract  = {Roussel Louis. — L'évolution récente de la structure des ménages dans quelques pays industriels. Comment la structure des ménages a-t-elle évolué dans les pays industriels depuis 1960 ? L'auteur se propose de répondre à cette question à partir d'un certain nombre de données, et spécialement des quatre monographies publiées récemment sur ce thème dans la revue Population. Il apparaît ainsi que se sont multipliés les ménages d'une personne, ceux formés par une famille monoparentale, et ceux de cohabitants non mariés. Plus généralement, on observe des tendances convergentes vers une distribution pluraliste sans modèle nettement dominant en perspective transversale, vers une rétrécissement de la taille, vers une plus grande mobilité des individus d'une unité à une autre. Ces convergences ne doivent pas masquer la persistance de comportements différents, en particulier dans la constitution des unions libres et de la fécondité hors-mariage. Longtemps tenue pour secondaire, l'étude des ménages se développe rapidement : les tentatives actuelles pour lier l'analyse des comportements individuels et celle des structures domestiques témoignent de cet intérêt nouveau.},
  annote    = {doi:10.2307/1532922http://www.persee.fr/articleAsPDF/pop\_0032-4663\_1986\_num\_41\_6\_17671/article\_pop\_0032-4663\_1986\_num\_41\_6\_17671.pdf?mode=light},
