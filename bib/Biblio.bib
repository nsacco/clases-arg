@Article{Grusky1998,
  author    = {Grusky, David B. and Sørensen, Jesper B.},
  journal   = {American Journal of Sociology},
  title     = {Can {Class} {Analysis} be {Salvaged}?},
  year      = {1998},
  issn      = {00029602},
  number    = {5},
  pages     = {1187},
  volume    = {103},
  abstract  = {The article focuses on class analysis that can be attributed to the declining appeal of aggregate representations of class coupled with the virtual absence of any disaggregate alternatives. The existing literature is short on such reconstructive efforts. At present, virtually all protagonists adopt one of three analytic positions, two of which arguably take contemporary anticlass critiques too seriously. The dominant form of revisionism, at least within American sociology, rests on the claim that long-standing cleavages of class, occupation, and status are giving way to new cleavages generated in institutional arenas that have become more prominent with the transition to post-industrialism. The "deconstructionists" have suggested, by contrast, that the prior reformulations over more fundamental declines in the effects of all structural cleavages. The most popular response to anticlass rhetoric has been to simply reaffirm the importance of class models, albeit sometimes with the understanding that such models apply in weakened form even after a good deal of refurbishing and revamping.},
  annote    = {Grusky, David B. 1; Sørensen, Jesper B. 2; Affiliations: 1 : Stanford University.; 2 : University of Chicago.; Source Info: Mar1998, Vol. 103 Issue 5, p1187; Thesaurus Term: SOCIAL classes; Thesaurus Term: CLASS analysis; Thesaurus Term: COMMUNIST revisionism; Thesaurus Term: INDUSTRIALISM; Thesaurus Term: CLEAVAGE (Social conflict); Thesaurus Term: SOCIOLOGY; Number of Pages: 48p; Illustrations: 1 Chart; Document Type: Article},
  keywords  = {CLASS analysis, CLEAVAGE (Social conflict), COMMUNIST revisionism, INDUSTRIALISM, SOCIAL classes, SOCIOLOGY},
  timestamp = {2020-09-30},
}

