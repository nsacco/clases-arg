  doi       = {10.1007/s11113-016-9405-1},
  file      = {Yoo-2017:files/23230/Yoo-2017.pdf:application/pdf},
  keywords  = {Fertility intention, Korea, Low fertility, Sex ratio at birth, Son preference},
  pmcid     = {PMC5272884},
