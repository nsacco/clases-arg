@Article{Prandy1999,
  author    = {Prandy, Kenneth},
  journal   = {International Journal of Sociology \& Social Policy},
  title     = {The social interaction approach to the measurment and analysis of social stratification},
  year      = {1999},
  issn      = {0144333X},
  number    = {9/10/11},
  pages     = {215--249},
  volume    = {19},
  abstract  = {This article presents an account of stratification arrangements, using the ideas of social interaction and social space. The author argues that such an account highlights more clearly than does class analysis the central role that is played in modern societies by an overall structure of inequality--a stratification order of generalized advantage--and by processes of its maintenance, reproduction and change. The key to understanding how it is possible to incorporate social judgments and so to conceptualize both a diversity of continuously-distributed resources and a relatively simple structure of stratification is the concept of social space. Class typologies each offer their own account of social space, but they share a view of it as divided into distinct sectors marked by homogeneity and similarity within the sectors and clear divisions between them. Most measures of class and stratification are essentially concerned with the distribution of resources. Good preliminary evidence for the significance of hierarchy can be seen by looking at the relation between a social interaction-based measure such as the Cambridge Scale and two examples of class models, the British Registrar-General's social classes and the Goldthorpe class schema. Social mobility, or more properly social reproduction, is a particular fruitful area in which to examine the nature of a social interaction-based measure. The analysis of patterns of social interaction indicates a uni-dimensional ordering of occupational groups, which are fairly evenly ranged, without any positive evidence for the existence of the kinds of discontinuities that would suggest class boundaries.},
  annote    = {Prandy, Kenneth 1; Affiliations: 1 : Professional Fellow, School of Social Sciences, Cardiff University; Source Info: 1999, Vol. 19 Issue 9/10/11, p215; Thesaurus Term: SOCIAL interaction; Thesaurus Term: SOCIAL space; Thesaurus Term: SOCIAL stratification; Thesaurus Term: SOCIAL mobility; Thesaurus Term: SOCIOLOGY; Number of Pages: 35p; Illustrations: 1 Chart; Document Type: Article},
  keywords  = {SOCIAL interaction, SOCIAL mobility, SOCIAL space, SOCIAL stratification, SOCIOLOGY},
  timestamp = {2020-09-30},
}

