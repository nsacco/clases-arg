  <locale xml:lang="en">
    <terms>
      <term form="short" name="editortranslator">
        <single>ed. &amp; trans.</single>
        <multiple>eds. &amp; trans.</multiple>
      </term>
      <term form="short" name="translator">
        <single>trans.</single>
        <multiple>trans.</multiple>
      </term>
    </terms>
  </locale>
  <locale xml:lang="es">
    <terms>
      <term form="short" name="month-01">Ene.</term>
      <term form="short" name="month-02">Feb.</term>
      <term form="short" name="month-03">Mar.</term>
      <term form="short" name="month-04">Abr.</term>
      <term form="short" name="month-05">May.</term>
      <term form="short" name="month-06">Jun.</term>
      <term form="short" name="month-07">Jul.</term>
      <term form="short" name="month-08">Ago.</term>
      <term form="short" name="month-09">Sept.</term>
      <term form="short" name="month-10">Oct.</term>
      <term form="short" name="month-11">Nov.</term>
      <term form="short" name="month-12">Dic.</term>
      <term name="retrieved">obtenido</term>
      <term name="from">de</term>
      <term form="short" name="editortranslator">
        <single>ed. &amp; trad.</single>
        <multiple>eds. &amp; trads.</multiple>
      </term>
      <term form="short" name="translator">
        <single>trad.</single>
        <multiple>trads.</multiple>
      </term>
      <term name="no date">s.f.</term>
